{
    "name": "yhjohn/color-tag",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Yhjohn\\ColorTag\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yhjohn\\ColorTag\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
